lets = c(letters,"_")
lets
count=c() //empty
for(i in 1:27)
+ {
+ count[i]=str_count(cat,lets[i])
+ }
count
lets[rev(order(count))]
answer=lets[rev(order(count))]
answer
w1=which(answer=="_")
answer2=answer[-c(w1:length(answer))]
answer2
 
